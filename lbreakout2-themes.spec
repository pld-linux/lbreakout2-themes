%prep
%setup -q -c

# included in base lbreakout2
%{__rm} absoluteB.zip moiree.zip oz.zip

%install
rm -rf $RPM_BUILD_ROOT
install -d $RPM_BUILD_ROOT%{_datadir}/lbreakout2/gfx

for f in *.zip ; do
	unzip "${f}" -d $RPM_BUILD_ROOT%{_datadir}/lbreakout2/gfx
done

%{__mv} $RPM_BUILD_ROOT%{_datadir}/lbreakout2/gfx/{"Technicolor Nostalgy",Technicolor_Nostalgy}

%clean
rm -rf $RPM_BUILD_ROOT

